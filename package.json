{
  "name": "elektreka",
  "version": "1.0.0",
  "description": "eCommerce application built with MERN stack",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder.js",
    "data:destroy": "node backend/seeder.js -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0-ANONU-0/Elektreka.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/0-ANONU-0/Elektreka/issues"
  },
  "homepage": "https://github.com/0-ANONU-0/Elektreka#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "express": "^4.18.2",
    "mongoose": "^8.0.4",
    "nodemon": "^3.0.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "dotenv": "^16.3.1"
  }
}
